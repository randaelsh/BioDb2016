    "def motif(seq1,seq2):\n",
    "    result = []\n",
    "    for i in range(0, len(seq1)-len(seq2)+1):\n",
    "        if seq1[i:i+len(seq2)] == seq2:\n",
    "            result.append(str(i+1))\n",
    "\n",
    "    print(' '.join(result))\n",
    "  \n",
    "    \n",
    "seq1='GATATATGCATATACTT'\n",
    "seq2='ATAT'\n",
    "motif(seq1,seq2)\n"
   ]
  }
