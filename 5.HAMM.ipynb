    "def HammingDist(s,t):\n",
    "    sequence=min(len(s), len(t))\n",
    "    distance=abs(len(s)-len(t))\n",
    "    for i in range(0,sequence):\n",
    "        if (s[i]!=t[i]): \n",
    "                distance+=1 \n",
    "    return(distance) \n",
    "       \n",
    "a=('GAGCCTACTAACGGGAT')\n",
    "b=('CATCGTAATGACGGCCT')\n",
    "HammingDist(a,b)"
   ]
  }
