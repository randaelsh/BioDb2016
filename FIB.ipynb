    "def Fib(n,k):\n",
    "\n",
    "    if n == 0:\n",
    "        return 0\n",
    "    elif  n == 1:\n",
    "        return 1\n",
    "    else:\n",
    "        return Fib(n-2,k)*k + Fib(n-1,k)\n",
    "Fib(5,3)"
   ]
