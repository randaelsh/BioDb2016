    "def transcribe(DNA):\n",
    "    DNAlist= list(DNA)\n",
    "    for i in range(len(DNAlist)):\n",
    "        if (DNAlist[i] == 'T'):\n",
    "                DNAlist[i] = 'U'\n",
    "    RNA=''.join(DNAlist)\n",
    "    return(RNA)\n",
    "    \n",
    "    \n",
    "\n",
    "transcribe('GATGGAACTTGACTACGTAAATT')"
   ]
